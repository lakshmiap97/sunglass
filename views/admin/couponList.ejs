                <button class="btn btn-icon btn-mobile me-auto" data-trigger="#offcanvas_aside"> <i
                        class="material-icons md-apps"></i> </button>
               
            </div>
        </header>

        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Coupens</h2>
                </div>
                <div>
                    <a href="addCoupon" class="btn btn-md rounded font-sm">Create New</a>
                </div>                
            </div>
            <div class="card mb-4">
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>Coupon code</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Min Discount</th>
                                    <th scope="col">Max Discount</th>
                                    <th scope="col">Min Purchase</th>
                                    <th scope="col">Coupon Expirey</th>
                                    <th scope="col">Status</th>
                                    <th scope="col" class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                <% coupon.forEach(el =>{%> 
                                <tr>
                                    <td><%= el.couponcode %></td>
                                    <td><b><%= el.name %></b></td>
                                    <td><%= el.discount %></td>
                                    <td><%= el.maxdiscount %></td>
                                    <td><%= el.minPurchase %></td>
                                    <td><%= el.expiryDate.toISOString().split('T')[0]  %></td>
                                    <% if(el.isActive == true){%>
                                    <td>Active</td>
                                    <%}else{%>
                                        <td>InActive</td>
                                        <%}%>
                                    <td class="text-end">
                                        <a href="/editCoupon?id=<%= el._id %>" class="btn btn-md rounded font-sm">Edit</a>
                                       
                                        <a href="/deleteCoupon?id=<%= el._id %>" class="btn btn-md rounded font-sm btn-danger">
                                            Delete
                                        </a>
                                        </a>
                                    </td>
                                </tr>
                                <% }) %>
                            </tbody>
                        </table>
                    </div> 
                </div>
            </div> 
            <div class="container mt-3">
                <nav aria-label="Page navigation">
                    <ul id="pagination" class="pagination justify-content-center mb-20" style="margin-right: 200px;">
                    <% if(locals.totalpage){%>
                        <% for (let i = 1; i <= totalpage; i++) { %>
                            <li class="page-item <%=(i == currentpage) ? 'active' : '' %>">
                                <a class="page-link" href="?page=<%= i %>"><%= i %></a>
                            </li>
                        <% } %>
                        <% }%>

                  
                         
                    </ul>
                </nav>
                </div>
