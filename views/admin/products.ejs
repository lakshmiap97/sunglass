    <main class="main-wrap">
        <header class="main-header navbar">
            <div class="col-search">
               
            </div>
            <div class="col-nav">
                <button class="btn btn-icon btn-mobile me-auto" data-trigger="#offcanvas_aside"> <i class="material-icons md-apps"></i> </button>
                <ul class="nav">
                    <li class="nav-item">
                        <!-- <a class="nav-link btn-icon" href="#">
                            <i class="material-icons md-notifications animation-shake"></i>
                            <span class="badge rounded-pill">3</span>
                        </a> -->
                    </li>
                    <li class="nav-item">
                        <a class="nav-link btn-icon darkmode" href="#"> <i class="material-icons md-nights_stay"></i> </a>
                    </li>
                    <li class="nav-item">
                        <a href="#" class="requestfullscreen nav-link btn-icon"><i class="material-icons md-cast"></i></a>
                    </li>
                    <li class="dropdown nav-item">
                        <a class="dropdown-toggle" data-bs-toggle="dropdown" href="#" id="dropdownLanguage" aria-expanded="false"><i class="material-icons md-public"></i></a>
                        
                    </li>
                    <li class="dropdown nav-item">
                        <a class="dropdown-toggle" data-bs-toggle="dropdown" href="#" id="dropdownAccount" aria-expanded="false"> <img class="img-xs rounded-circle" src="assets/imgs/people/avatar2.jpg" alt="User"></a>
                        <div class="dropdown-menu dropdown-menu-end" aria-labelledby="dropdownAccount">
                            
                            <a class="dropdown-item text-danger" href="/adminlogout"><i class="material-icons md-exit_to_app"></i>Logout</a>
                        </div>
                    </li>
                </ul>
            </div>
        </header>
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Products List</h2>
                    
                </div>
                <div class="col-search">
                    <form action="/product" method="get" class="searchform">
                        <div class="input-group">
                            <input list="search_terms" type="text" name="search" class="form-control" placeholder="Search term">
                            <button class="btn btn-light bg" type="submit"> <i class="material-icons md-search"></i></button>
                        </div>
                    </form>
                </div>
                
            </div>
            <div class="card mb-4">
                <div class="table-responsive">
                    <table  class="table table-hover"> 
                        <thread>
                            <tr>
                                <th>Name</th>
                                <th>Category</th>
                                <th>Sale Price</th>
                                <th>Status</th>
                                <th>Action</th>
                            </tr>
                        </thread>
                        <% if(product){%>
                        <% product.forEach (element =>{%>
                        <thread>
                            <tr>
                                <th><img src="/uploads/product-images/<%= element.image[0] %>" class="img-sm img-thumbnail" alt="Item">  <%= element.name %></th>
                                <th><%=element.category.name %></th>
                                <th><%= element.price.salesPrice %></th>
                                <th><% if(element.isActive==true){ %>
                                            <span class="badge rounded-pill alert-success">Active</span>
                                        <% } else { %>
                                            <span class="badge rounded-pill alert-danger">Inactive</span>
                                        <% } %></th>
                                <th><a href="/editProduct?_id=<%=element._id%>"><button  class="btn  btn-brand rounded font-sm mt-15">Edit</button></a>
                                        <a href="/productStatus?_id=<%= element._id %>"><button   class="btn  btn-brand rounded font-sm mt-15">
                                            <% if(element.isActive==true){%>
                                                Block
                                            <% } else { %>
                                                Unblock
                                            <% } %>
                                        </button></th>
                            </tr>
                        </thread>
                         <%})%>
                    <%}%>
                    </table>
               </div>
            </div> <!-- card end// -->
            <div class="container mt-3">
                <nav aria-label="Page navigation">
                    <ul id="pagination" class="pagination justify-content-center mb-20" style="margin-right: 200px;">
                    <% if(locals.totalpage){%>
                        <% for (let i = 1; i <= totalpage; i++) { %>
                            <li class="page-item <%=(i == currentpage) ? 'active' : '' %>">
                                <a class="page-link" href="?page=<%= i %>"><%= i %></a>
                            </li>
                        <% } %>
                        <% }%>

                  
                         
                    </ul>
                </nav>
                </div>
        </section> <!-- content-main end// -->
        <footer class="main-footer font-xs">
