                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>ID</th>
                                    <th scope="col">Name</th>
                                    <th ></th>
                                    <th scope="col">Price</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Date</th>
                                    <th scope="col" class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                <% order.forEach(element=>{%> 
                                <tr>
                                    <td><%= element._id %></td>
                                    <td><b><%= element.address.name %></b></td>
                                    <td></td>
                                    <td><%= element.totalamount %></td>
                                    <% if(element.status=="pending"){%>   
                                    <td><span class="badge rounded-pill alert-warning"><%= element.status %></span></td>
                                    <%}else if(element.status=="confirmed"){%>
                                        <td><span class="badge rounded-pill alert-success"><%= element.status %></span></td>
                                     <%}else{%>   
                                        <td><span class="badge rounded-pill alert-warning"><%= element.status %></span></td>
                                     <%}%>
                                    <td><%= new Date(element.createdAt).toLocaleDateString() %></td>
                                    <td class="text-end">
                                        <a href="orderdetails?id=<%= element._id %>" class="btn btn-md rounded font-sm">Detail</a>
                                        <!-- <div class="dropdown">
                                            <a href="#" data-bs-toggle="dropdown" class="btn btn-light rounded btn-sm font-sm"> <i class="material-icons md-more_horiz"></i> </a>
