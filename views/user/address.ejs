<!-- Display Billing Address -->
<!-- Display Billing Address -->
<div class="myaccount-content">
    <h5>Billing Address</h5>
    <% if(addresses && addresses.addresses && addresses.addresses.length > 0) { %>
        <% addresses.addresses.forEach(element => { %>
            <div class="card mb-3 mb-lg-0">
                <div class="card-header">
                    <h5 class="mb-0">Billing Address</h5>
                </div>
                <div class="card-body">
                    <address>
                        <p><strong><%= element.name %></strong></p>
                        <p><%= element.address %>, <%= element.city %> <br>
                            <%= element.state %>, <%= element.pincode %></p>
                        <p><%= element.mobile %></p>
                    </address>
                    <!-- Wrap the buttons in a div with d-flex justify-content-between classes -->
                    <div class="d-flex justify-content-between align-items-center">
                        <div>
                            <a href="/editAddress?_id=<%= element._id %>" class="btn btn-primary"><i class="fa fa-edit"></i> Edit</a>
                        </div>
                        <div>
                            <form action="/deleteAddress" method="post" class="delete-address-form">
                                <input type="hidden" name="addressId" value="<%= element._id %>">
                                <button type="submit" class="btn btn-danger delete-address-btn"><i class="fa fa-trash"></i> Delete</button>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        <% }); %>
    <% } else { %>
        <p>No billing addresses found.</p>
    <% } %>
    <!-- Always display the "Add Address" button -->
    <a href="/addaddress" class="btn btn-sqr"><i class="fa fa-edit"></i> Add Address</a>
</div>


                                
                            </div>
                            <!-- Single Tab Content End -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
