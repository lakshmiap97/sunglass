     <script>
        // Wait for the DOM to be fully loaded
document.addEventListener('DOMContentLoaded', function() {
    // Get all elements with the class 'pe-7s-search'
    const searchIcons = document.querySelectorAll('.pe-7s-search');

    // Iterate over each search icon
    searchIcons.forEach(function(icon) {
        // Attach a click event listener
        icon.addEventListener('click', function(event) {
            event.preventDefault(); // Prevent the default link behavior

            // Extract the product ID from the href attribute
            const productId = this.parentElement.getAttribute('href').split('=')[1];

            // Make an AJAX request or perform any necessary action
            // to load the quick details for the product with this ID
            loadQuickDetails(productId);
        });
    });
}); 

function loadQuickDetails(productId) {
    // Assuming you're using jQuery for AJAX, perform an AJAX request
    $.ajax({
        url: `/quickDetails?id=${productId}`,
        method: 'GET',
        success: function(response) {
            // Assuming response contains HTML for the quick details modal content
            // Update the content of the modal with the received data
            $('#quick_view .modal-body').html(response);
            $('#quick_view').modal('show'); // Show the modal
        },
        error: function(err) {
            console.error('Error fetching quick details:', err);
        }
    });
}

    </script>

