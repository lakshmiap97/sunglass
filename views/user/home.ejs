/* styles.css */
.alert {
    padding: 15px;
    margin-bottom: 20px;
    border: 1px solid transparent;
    border-radius: 4px;
    position: relative;
    background-color: #f8d7da; /* Background color */
    color: #721c24; /* Text color */
    border-color: #f5c6cb; /* Border color */
}

.alert-warning {
    background-color: #fff3cd; /* Background color */
    color: #856404; /* Text color */
