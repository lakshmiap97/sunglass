<script>
    $(document).ready(function() {
       
        const minPrice = parseInt($('.price-range').data('min'));
        const maxPrice = parseInt($('.price-range').data('max'));
        
        $('.price-range').slider({
            range: true,
            min: minPrice,
            max: maxPrice,
            values: [minPrice, maxPrice],
            slide: function(event, ui) {
                $('#amount').val('$' + ui.values[0] + ' - $' + ui.values[1]);
            }
        });
        
        
        $('#amount').val('$' + minPrice + ' - $' + maxPrice);
        
       
        $('.filter-btn').on('click', function(event) {
            event.preventDefault();
            
            const minPrice = $('.price-range').slider('values', 0);
            const maxPrice = $('.price-range').slider('values', 1);
            
            
            filterProductsByPrice(minPrice, maxPrice);
        });
        
       
        function filterProductsByPrice(minPrice, maxPrice) {
            console.log('Filtering products with price range:', minPrice, '-', maxPrice);
            
        }
    });
</script>

