                                                                    <% paginatedData.forEach(element => { %> 
                                                                    <tr>
                                                                        <td><%= element.transactionID %></td>
                                                                        <td><%= element.history %></td>
                                                                        <td><%= new Date(element.date).toLocaleDateString() %></td>
                                                                    </tr> 
                                                                    <% }) %>
                                                                <% } else { %>
                                                                    <tr>
                                                                        <td colspan="3">No transactions available.</td>
                                                                    </tr>
                                                                <% } %>
                                                            </tbody>
                                                        </table>
                                                        <% } else { %>
                                                            <p>No wallet data available.</p>
                                                        <% } %>
