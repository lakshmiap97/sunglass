                                                        <% if (wallet) { %>
                                                        <table class="table">
                                                            <thead>
                                                                <tr>
                                                                    <th>Transaction ID</th>
                                                                    <th>Amount</th>
                                                                    <th>Date</th>
                                                                </tr>
