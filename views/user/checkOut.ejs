    <script>
        async function paymentFailed(orderId) {
    const cartID = document.querySelector('input[name="cartID"]').value;
    const radiobtn = document.querySelectorAll('.radiobtn');
    const paymentMethod = document.querySelector('input[name="paymentMethod"]:checked').value;
    const totalPrice = document.querySelector('input[name="totalPrice"]').value;
    const addressID = document.querySelector('input[name="addressID"]').value;
    const coupon = document.getElementById('coup').value;

    let radiovalue;
    radiobtn.forEach(element => {
        if (element.checked) {
            radiovalue = element.value;
        }
    });

    fetch('/paymentfailed', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json'
        },
        body: JSON.stringify({ orderId, cartID, radiovalue, totalPrice, coupon, paymentMethod, addressID })
    })
    .then(response => response.json())
    .then(data => {
        if (data.success) {
            window.location.reload();
        } else {
            alert('Payment failed! Please try again.');
        }
    });
}

        document.getElementById('orderForm').onsubmit = async function (e) {
            e.preventDefault();
    
            const paymentMethod = document.querySelector('input[name="paymentMethod"]:checked').value;
            const cartID = document.querySelector('input[name="cartID"]').value;
            const totalPrice = document.getElementById('totalPrice').textContent.replace('₹', ''); // Fetch the updated total price
            const productsID = document.querySelector('input[name="productsID"]').value;
            const addressID = document.querySelector('input[name="addressID"]').value;
    
            if (paymentMethod === 'razorpay') {
                // Create Razorpay order
                const response = await fetch('/createOrder', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({ totalPrice, cartID, addressID })
                });
                const order = await response.json();
    
                // Initialize Razorpay payment
                const options = {
                    "key": 'rzp_test_8oA5wZD2fGxjcW',
                    "amount": order.orderID.amount,
                    "currency": "INR",
                    "name": "myshade",
                    "description": "Order Payment",
                    "image": "https://example.com/your_logo",
                    "order_id": order.orderID.id,
                    "handler": async function (response) {
                        // Handle Razorpay payment success
                        const paymentResponse = await fetch('/paymentsuccess', {
                            method: 'POST',
                            headers: {
                                'Content-Type': 'application/json'
                            },
                            body: JSON.stringify({
                                paymentid: response.razorpay_payment_id,
                                razorpayorderid: response.razorpay_order_id,
                                signature: response.razorpay_signature,
                                orderid: order.orderID.id
                            })
                        });
                        const result = await paymentResponse.json();
    
                        if (result.success) {
                            // Place order after successful payment
                            const placeOrderResponse = await fetch('/placeOrder', {
                                method: 'POST',
                                headers: {
                                    'Content-Type': 'application/json'
                                },
                                body: JSON.stringify({ cartID, totalPrice, paymentMethod, productsID, addressID })
                            });
                            const placeOrderResult = await placeOrderResponse.json();
    
                            if (placeOrderResult.success) {
                                window.location.href = '/orderSuccess';
                            } else {
                                alert('Order placement failed! Please try again.');
                            }
                        } else {
                            alert('Payment verification failed! Please try again.');
                            console.error('Payment failed:', result);
                            paymentFailed(order.orderID.id); // Call paymentFailed function
                        }
                    },
                    "prefill": {
                        "name": "Lakshmi A P",
                        "email": "lakshmiap97@gmail.com",
                        "contact": "9999999999"
                    },
                    "notes": {
                        "address": "Your Address"
                    },
                    "theme": {
                        "color": "#F37254"
                    },
                    "modal": {
                        "ondismiss": function () {
                            paymentFailed(order.orderID.id); // Call paymentFailed function on modal dismiss
                        }
                    }
                };
                var rzp1 = new Razorpay(options);
                rzp1.open();
            } else if (paymentMethod === 'cash' || paymentMethod === 'COD' || paymentMethod === 'wallet') {
                if (paymentMethod === 'cash' && totalPrice > 1000) {
                    alert('Cash on Delivery is not available for amounts greater than 1000.');
                   
                    paymentFailed(null); // Call paymentFailed function with no orderId
                    return; // Exit the function
                }
    
                const placeOrderResponse = await fetch('/placeOrder', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({ cartID, paymentMethod, totalPrice, productsID, addressID })
                });
                const placeOrderResult = await placeOrderResponse.json();
    
                if (placeOrderResult.success) {
                    window.location.href = '/orderSuccess';
                } else {
                    alert('Order placement failed! Please try again.');
                }
            } else {
                alert('Please select a payment method.');
            }
        };
    </script>
    
    


    <script>


document.addEventListener('DOMContentLoaded', () => {
    const couponButtons = document.querySelectorAll('.apply-coupon-btn');
    couponButtons.forEach(button => {
        button.addEventListener('click', () => {
            const couponCode = button.getAttribute('data-couponcode');
            document.getElementById('coup').value = couponCode;
        });
