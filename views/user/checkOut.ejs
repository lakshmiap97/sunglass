});

async function applyCoupon(subTotal, cartID) {
    const couponcode = document.querySelector('#coup').value;

    try {
        const response = await fetch('/apply-coupon', {
            method: 'post',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({ couponcode, subTotal, cartID })
        });

        const data = await response.json();
        console.log('datas', data);
        if (response.ok) {
            // Update discount and total amounts
            document.getElementById('discountAmount').textContent = `₹${data.discountAmount}`;
            document.getElementById('totalPrice').textContent = `₹${data.discounted}`;
            document.getElementById('message').textContent = '';

            // Switch buttons
            document.getElementById('applyCouponButton').style.display = 'none';
            document.getElementById('removeCouponButton').style.display = 'block';
            
        } else {
            document.getElementById('message').textContent = data.message;
        }
    } catch (error) {
        console.error('Error:', error);
        document.getElementById('message').textContent = 'An error occurred while applying the coupon';
    }
}

async function removeCoupon(cartID) {
    try {
        const response = await fetch('/remove-coupon', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({ cartID })
        });

        const data = await response.json();
        if (response.ok) {
            // Reset discount and total amounts
            document.getElementById('discountAmount').textContent = '₹0.00';
            
            // Update total price with the original subtotal
            document.getElementById('totalPrice').textContent = `₹${data.subTotal}`;
            document.getElementById('message').textContent = data.message;

            // Switch buttons
            document.getElementById('applyCouponButton').style.display = 'block';
            document.getElementById('removeCouponButton').style.display = 'none';
            document.getElementById('coup').placeholder = 'Enter Coupon Code...'; // Reset placeholder
            document.getElementById('coup').value = ''; // Clear input field
        } else {
            document.getElementById('message').textContent = data.message;
        }
    } catch (error) {
        console.error('Error:', error);
        document.getElementById('message').textContent = 'An error occurred while removing the coupon';
    }
}


</script>

