        <section class="mt-50 mb-50">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="table-responsive">
                            <% if (!wishlist || wishlist.items.length <= 0) { %>
                                <div class="mb-150 text-center">
                                    <h1 class="text-dark">No items in wishlist</h1>
                                </div>
                            <% } else { %>
                                <table class="table shopping-summery text-center mb-100">
                                    <thead>
                                        <tr class="main-heading">
                                            <th scope="col" colspan="2">Product</th>
                                            <th scope="col">Price</th>
                                            <th scope="col">Stock Status</th>
                                            <th scope="col">Action</th>
                                            <th scope="col">Remove</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% wishlist.items.forEach(wishlistItem => { %>
                                            <% const product = wishlistItem.product; %>
                                            <tr>
                                                <td class="image product-thumbnail">
                                                    <% if (product.image && product.image.length > 0) { %>
                                                        <img src="/uploads/product-images/<%= product.image[0] %>" alt="#">
                                                    
                                                    <% } %>
                                                   
                                                </td>
                                                <td class="product-des product-name">
                                                    <h5 class="product-name"><a href="shop-product-right.html"><%= product.name %></a></h5>
                                                </td>
                                                <td class="price" data-title="Price"><span>â‚¹<%= product.price.salesPrice %></span></td>
                                                <% if (product.totalQuantity > 0) { %>
                                                    <td class="text-center" data-title="Stock">
                                                        <span class="text-success font-weight-bold">In Stock</span>
                                                    </td>
                                                <% } else { %>
                                                    <td class="text-center" data-title="Stock">
                                                        <span class="text-danger font-weight-bold">Out of Stock</span>
                                                    </td>
                                                <% } %>
                                                <td class="text-center" data-title="Cart">
                                                    <button type="button" class="btn btn-sm btn-btn-fill-out-submit" data-toggle="modal" data-target="#colorModal_<%= product._id %>">Add to cart</button>
                                                </td>
                                                <td class="action" data-title="Remove">
                                                    <button class="btn btn-sm btn-danger" type="button" onclick="removeFromWishlist('<%= product._id %>')">Delete</button>
                                                </td>
                                            </tr>
                                        <% }); %>
                                    </tbody>
                                </table>
                            <% } %>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        
        <!-- Color Modals -->
        <% wishlist.items.forEach(wishlistItem => { %>
            <% const product = wishlistItem.product; %>
            <div class="modal" id="colorModal_<%= product._id %>">
                <div class="modal-content">
                    <span class="close" onclick="closeColorModal('colorModal_<%= product._id %>')">&times;</span>
                    <h2>Select Color</h2>
                    <label for="color-select_<%= product._id %>">Select Color:</label>
                    <select id="color-select_<%= product._id %>" onchange="findColor(this.value, '<%= product._id %>')">
                        <option value="" disabled selected>Select Color</option>
                        <% Object.keys(product.color).forEach(color => { %>
                            <option value="<%= color %>"><%= color %></option>
                        <% }); %>
                    </select>
                    <p id="stock-status_<%= product._id %>"></p>
                    <button class="btn btn-sm btn-btn-fill-out-submit" type="submit" value="submit" onclick="addToCart('<%= product._id %>')">Add to cart</button>
                </div>
            </div>
        <% }); %>
        
        
        
